type Query {
    getProducts: [Product]
    getProduct(productId: String!): Product

    getDivers: [Diver]
    getDiver(diverId: String!): Diver
}

type Product {
    seq: ID!
    productId: String!
    productName: String!
    productDescription: String!
    deleteFlag: Boolean!
}